{
  "name": "maree-lettres-shared",
  "private": true,
  "version": "0.2.2",
  "description": "shared code for MareeLettres",
  "main": "lib/maree-lettres-shared.js",
  "scripts": {
    "build": "webpack --env build",
    "dev": "webpack --progress --colors --watch --env dev",
    "install-server": "cd ../../meteor; npm run install-shared",
    "install-app": "cd ../../react-native; yarn run install-shared",
    "install-simulator": "cd ../../simulator; yarn run install-shared",
    "test": "mocha --compilers js:babel-core/register --colors ./test/*.spec.js",
    "test:watch": "mocha --compilers js:babel-core/register --colors -w ./test/*.spec.js"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.0.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-preset-es2015": "6.24.1",
    "chai": "3.5.0",
    "eslint": "3.19.0",
    "eslint-loader": "1.7.1",
    "mocha": "3.3.0",
    "webpack": "3.1.0",
    "yargs": "7.1.0"
  },
  "dependencies": {
    "cryptr": "^2.0.0",
    "md5": "^2.2.1"
  },
  "license": "no licence"
}
